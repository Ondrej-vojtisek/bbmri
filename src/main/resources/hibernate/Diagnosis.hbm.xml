<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.Diagnosis" table="diagnosis" lazy="true">
		<composite-id>
			<key-property name="key" column="`key`" type="text"/>
			<key-many-to-one name="sample" column="sample_id" class="cz.bbmri.entity.Sample">
			</key-many-to-one>
		</composite-id>
	</class>
</hibernate-mapping>
