<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.Quantity" table="quantity" lazy="true">
		<id name="sampleId" column="sample_id" type="long">
			<generator class="foreign">
				<param name="property">sample</param>
			</generator>
		</id>
		<one-to-one name="sample" class="cz.bbmri.entity.Sample" cascade="all" constrained="true"/>
		<property name="available" column="available" type="short" not-null="true" lazy="false"/>
		<property name="total" column="total" type="short" not-null="true" lazy="false"/>
		<property name="original" column="original" type="short" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
