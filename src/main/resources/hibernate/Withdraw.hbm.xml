<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.Withdraw" table="withdraw" lazy="true">
		<id name="id" column="id" type="long" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">withdraw_seq</param>
			</generator>
		</id>
		<many-to-one name="biobank" cascade="lock" column="biobank_id" class="cz.bbmri.entity.Biobank" not-null="true" lazy="proxy">
		</many-to-one>
		<property name="created" column="created" type="timestamp" not-null="false" lazy="false"/>
		<set name="request" lazy="extra" cascade="save-update,lock" inverse="true">
			<key column="withdraw_id" not-null="false"/>
			<one-to-many class="cz.bbmri.entity.Request"/>
		</set>
	</class>
</hibernate-mapping>
