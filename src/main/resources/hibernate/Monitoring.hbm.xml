<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.Monitoring" table="monitoring" lazy="true">
		<id name="id" column="id" type="long" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">monitoring_seq</param>
			</generator>
		</id>
		<many-to-one name="container" cascade="lock" column="container_id" class="cz.bbmri.entity.Container" not-null="true" lazy="proxy">
		</many-to-one>
		<property name="name" column="name" type="text" not-null="false" lazy="false"/>
		<many-to-one name="measurementType" cascade="lock" column="measurement_type_id" class="cz.bbmri.entity.MeasurementType" not-null="true" lazy="proxy">
		</many-to-one>
		<set name="record" lazy="extra" cascade="save-update,lock" inverse="true">
			<key column="monitoring_id" not-null="true"/>
			<one-to-many class="cz.bbmri.entity.Record"/>
		</set>
	</class>
</hibernate-mapping>
