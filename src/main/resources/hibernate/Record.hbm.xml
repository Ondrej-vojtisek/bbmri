<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.Record" table="record" lazy="true">
		<composite-id>
			<key-many-to-one name="monitoring" column="monitoring_id" class="cz.bbmri.entity.Monitoring">
			</key-many-to-one>
			<key-property name="time" column="time" type="timestamp"/>
		</composite-id>
		<property name="value" column="value" type="double" length="10" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
