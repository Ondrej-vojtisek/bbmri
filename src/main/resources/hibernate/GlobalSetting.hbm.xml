<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.GlobalSetting" table="global_setting" lazy="true">
		<id name="key" column="`key`" type="text">
			<generator class="native">
			</generator>
		</id>
		<property name="value" column="value" type="text" not-null="false" lazy="false"/>
		<property name="lastModification" column="last_modification" type="date" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
