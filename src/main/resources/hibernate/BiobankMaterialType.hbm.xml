<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.bbmri.entity.BiobankMaterialType" table="biobank_material_type" lazy="true">
		<composite-id>
			<key-many-to-one name="biobank" column="biobank_id" class="cz.bbmri.entity.Biobank">
			</key-many-to-one>
			<key-many-to-one name="materialType" column="material_type_id" class="cz.bbmri.entity.MaterialType">
			</key-many-to-one>
			<key-property name="key" column="`key`" type="text"/>
		</composite-id>
	</class>
</hibernate-mapping>
